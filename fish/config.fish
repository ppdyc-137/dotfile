if status is-interactive
    # Commands to run in interactive sessions can go here

    set -gx EDITOR nvim
    # set -u pure_show_system_time true

    alias l="ls -l"
    alias la="ls -la"

    alias q="exit"
    alias clr="clear"

    alias vim="nvim"
    alias vi="nvim"
    alias df="duf"
    alias cat="bat"
    alias du="gdu"
    alias top="htop"

    alias n="neofetch"
    alias cw="cowsay"
    alias ra="joshuto"

end
